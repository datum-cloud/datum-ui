{
  "name": "datum-ui",
  "version": "1.0.0",
  "description": "A monorepo for Datum UI apps and supporting libraries",
  "repository": "git@github.com:datumforge/datum-ui.git",
  "author": "datum <info@datum.net>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "type-check": "turbo type-check",
    "clean": "rm -rf node_modules/** && rm -rf ./apps/operator/.next && rm -rf ./apps/docs/.next && rm -rf ./apps/web/.next && turbo clean",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "latest",
    "eslint": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.0.15",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "trustedDependencies": [
    "core-js"
  ],
  "resolutions": {
    "react": "18.3.1"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.4.4"
  }
}
