{
  "name": "datum-ui",
  "version": "1.0.0",
  "description": "A monorepo for Datum UI apps and supporting libraries",
  "repository": "git@github.com:datumforge/datum-ui.git",
  "author": "datum <info@datum.net>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "type-check": "turbo type-check",
    "clean": "rm -rf node_modules/** && rm -rf ./apps/operator/.next && rm -rf ./apps/docs/.next && turbo clean",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "latest",
    "eslint": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.0.15",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": { "@jsonforms/core": "^3.2.1", "@jsonforms/react": "^3.2.1", "@jsonforms/vanilla-renderers": "^3.2.1", "@optum/json-schema-editor": "^2.1.0", "@react-pdf/renderer": "^3.4.2", "form-pdf2json": "^0.1.6", "lucide-react": "^0.366.0", "pdfmake": "^0.2.10", "react-to-pdf": "^1.0.1" }
}
